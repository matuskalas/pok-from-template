is_a: EDAM:0002357 ! Protein signature type
relationship: is_identifier_of EDAM:0000008 ! Undefined

[Term]
id: EDAM:0001375
name: Protein repeat signature
namespace: data
def: "A protein repeat signature (sequence classifier) from the InterPro database." [EDAM:EBI "EMBRACE definition"]
